{
  "name": "svelte-store-tree",
  "version": "0.3.1",
  "type": "module",
  "description": "Provides writable/readable stores that can 'zoom' into the part of the store value (nested stores).",
  "main": "dist/src/index.js",
  "files": [
    "dist/src/*",
    "docs/*",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint --ext svelte,ts src test example",
    "fix:lint": "eslint --ext svelte,ts --fix src test example",
    "test": "vitest",
    "test:detailed": "vitest --reporter verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igrep/svelte-store-tree.git"
  },
  "keywords": [
    "svelte",
    "state"
  ],
  "author": "Internet Initiative Japan Inc.",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/igrep/svelte-store-tree/issues"
  },
  "homepage": "https://github.com/igrep/svelte-store-tree#readme",
  "devDependencies": {
    "svelte": "^3.50.0",
    "@sveltejs/vite-plugin-svelte": "^1.0.5",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte": "^2.8.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte-check": "^2.9.0",
    "svelte-eslint-parser": "^0.18.2",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2",
    "vite": "^3.1.0",
    "vitest": "^0.23.1"
  }
}
