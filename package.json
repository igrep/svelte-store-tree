{
  "name": "svelte-store-tree",
  "version": "0.5.1",
  "type": "module",
  "description": "Provides writable/readable stores that can 'zoom' into the part of the store value (nested stores).",
  "main": "src/index.js",
  "files": [
    "src/*",
    "docs/*",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint --ext svelte,ts src test example",
    "fix:lint": "eslint --ext svelte,ts --fix src test example",
    "test": "vitest",
    "test:detailed": "vitest --reporter verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igrep/svelte-store-tree.git"
  },
  "keywords": [
    "svelte",
    "state"
  ],
  "author": "Internet Initiative Japan Inc.",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/igrep/svelte-store-tree/issues"
  },
  "homepage": "https://github.com/igrep/svelte-store-tree#readme",
  "devDependencies": {
    "svelte": "^4.0.5",
    "@sveltejs/vite-plugin-svelte": "^2.4.3",
    "@tsconfig/svelte": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-svelte": "^2.32.4",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte-check": "^3.4.6",
    "svelte-eslint-parser": "^0.32.2",
    "svelte-preprocess": "^5.0.4",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vitest": "^0.33.0"
  }
}
